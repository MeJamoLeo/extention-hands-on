{
  "manifest_version": 2,
  "name": "History Viewer",
  "version": "0.2.1",
  "description": "Show your browser history",
  "author": "Your name",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon38.png",
    "default_title": "History Viewer",
    "default_popup": "popup.html"
  },
  "permissions": [
    "history"
  ]
}